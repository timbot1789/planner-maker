{
  "name": "@lit/lit-starter-ts",
  "private": true,
  "version": "2.0.1",
  "description": "A simple web component",
  "main": "my-element.js",
  "module": "my-element.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "pod": "community-solid-server -c config/solid-config.json -p 3001 -f data/",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "build:ldo": "ldo build --input src/.shapes --output src/.ldo",
    "dev": "vite --port 4040"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Google LLC",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@fullcalendar/core": "^6.1.11",
    "@fullcalendar/daygrid": "^6.1.11",
    "@fullcalendar/list": "^6.1.11",
    "@fullcalendar/timegrid": "^6.1.11",
    "@inrupt/solid-client-authn-browser": "^2.1.0",
    "@ldo/ldo": "^0.0.1-alpha.24",
    "@ldo/solid": "^0.0.1-alpha.24",
    "@ldo/solid-react": "^0.0.1-alpha.24",
    "@lit/context": "^1.1.0",
    "@solid/community-server": "^7.0.4",
    "@types/react": "^18.2.72",
    "lit": "^3.0.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@ldo/cli": "^0.0.1-alpha.24",
    "@open-wc/testing": "^3.1.5",
    "@types/jsonld": "^1.5.13",
    "@types/node": "^15.12.1",
    "@types/shexj": "^2.1.7",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "ldo-cli": "^3.0.1",
    "lit-analyzer": "^2.0.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "~5.3.3",
    "vite": "^2.3.6",
    "vite-plugin-node-polyfills": "^0.21.0"
  },
  "customElements": "custom-elements.json"
}
